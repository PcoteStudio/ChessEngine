{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["strict", "strict/mocha"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "linebreak-style": "off",
    "no-process-env": "off",
    "no-console": "off",
    "init-declarations":"off"
  }
}
